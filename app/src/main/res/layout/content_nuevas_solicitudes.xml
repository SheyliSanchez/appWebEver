<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="unah.proyecto.aeo.aplicacionagendaelectronicaoriental.clasesJAVAMelvin.AdministracionDePerfilesAdmin.NuevasSolicitudes"
    tools:showIn="@layout/app_bar_nuevas_solicitudes"
    >

    <ListView
        android:id="@+id/listviewperfilesPendientes"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@null"
        android:dividerHeight="0dp"
        />
    <ProgressBar
        android:layout_centerInParent="true"
        android:id="@+id/progressBarPerfilesPendientes"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center"
        />


</RelativeLayout>